{
  "name": "kissanmarketindia",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "migrate-gen": "npx sequelize-cli migration:generate --name $npm_config_name --migrations-path ./DataStore/migrations",
    "migrate-up": "npx sequelize-cli --config ./DataStore/config/config.json db:migrate --migrations-path ./DataStore/migrations",
    "migrate-down": "npx sequelize-cli --config ./DataStore/config/config.json db:migrate:undo:all --migrations-path ./DataStore/migrations",
    "seed-up": "npx sequelize-cli --config ./DataStore/config/config.json db:seed:all --seeders-path ./DataStore/seeders",
    "seed-down": "npx sequelize-cli --config ./DataStore/config/config.json db:seed:undo:all --seeders-path ./DataStore/seeders",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gokulkrh/kissanmarketindia.git"
  },
  "author": "Gokul Krishnan G",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gokulkrh/kissanmarketindia/issues"
  },
  "homepage": "https://github.com/gokulkrh/kissanmarketindia#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mysql2": "^3.3.3",
    "nodemon": "^2.0.22",
    "sequelize": "^6.31.1",
    "sequelize-cli": "^6.6.0"
  }
}
